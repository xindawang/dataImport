CREATE TABLE DATADIC.DATA_DIC
(
  ID INT PRIMARY KEY,
  DATA_ELEMENT_NAME VARCHAR2(60),
  DATA_ELEMENT_NAME_CHN VARCHAR2(100),
  TEI_OR_ACRONYM VARCHAR2(4),
  FORMAT VARCHAR2(10),
  XML_DATA_TYPE VARCHAR2(16),
  XML_DATA_TYPE_CHN VARCHAR2(16),
  SUB_DATA_ELEMENTS VARCHAR2(20),
  SUB_DATA_ELEMENTS_CHN VARCHAR2(20),
  ATTRIBUTES VARCHAR2(20),
  ATTRIBUTES_CHN VARCHAR2(30),
  USAGE VARCHAR2(110),
  USAGE_CHN VARCHAR2(150),
  DESCRIPTION_OR_PURPOSE VARCHAR2(600),
  DESCRIPTION_OR_PURPOSE_CHN VARCHAR2(600),
  CODES VARCHAR2(1100),
  CODES_CHN VARCHAR2(1500),
  REMARKS CLOB,
  REMARKS_CHN CLOB,
  EXAMPLES CLOB,
  EXAMPLES_CHN CLOB
);


INSERT INTO DATA_DIC(ID,DATA_ELEMENT_NAME,TEI_OR_ACRONYM,FORMAT,XML_DATA_TYPE,SUB_DATA_ELEMENTS,
                  ATTRIBUTES,USAGE,DESCRIPTION_OR_PURPOSE,CODES,REMARKS,EXAMPLES)
SELECT ID,DATA_ELEMENT_NAME,TEI_OR_ACRONYM,FORMAT,XML_DATA_TYPE,SUB_DATA_ELEMENTS,
ATTRIBUTES,USAGE,DESCRIPTION_OR_PURPOSE,CODES,REMARKS,EXAMPLES FROM DATA_DIC_ENG ORDER BY ID;


UPDATE DATA_DIC
SET (DATA_ELEMENT_NAME_CHN,XML_DATA_TYPE_CHN,SUB_DATA_ELEMENTS_CHN,ATTRIBUTES_CHN,USAGE_CHN,DESCRIPTION_OR_PURPOSE_CHN,CODES_CHN,REMARKS_CHN,EXAMPLES_CHN)=
(SELECT DATA_ELEMENT_NAME,XML_DATA_TYPE,SUB_DATA_ELEMENTS,ATTRIBUTES,USAGE,DESCRIPTION_OR_PURPOSE,CODES,REMARKS,EXAMPLES
 FROM DATA_DIC_CHN
 WHERE DATA_DIC.Id=DATA_DIC_CHN.Id);